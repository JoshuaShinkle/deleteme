<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
        android:background="@color/you_color_hint"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:guideline_height="492"
        app:guideline_width="1080">
        <HorizontalScrollView
            android:background="#ffffff"
            android:paddingTop="2dp"
            android:paddingBottom="2dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:orientation="horizontal"
                android:background="@color/you_color_hint"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:divider="@drawable/padding_chat_plus_column"
                android:showDividers="middle">
                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    app:guideline_height="492"
                    app:guideline_width="300">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:divider="@drawable/padding_chat_plus_row"
                        android:showDividers="middle">
                        <RelativeLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/camera"
                            android:background="@drawable/image_selector_press_30_percentage_dark"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    app:guideline_height="125"
                                    app:guideline_width="268"/>
                                <TextView
                                    android:textColor="#000000"
                                    android:text="@string/camera"
                                    style="@style/PlusFragmentButtonText"/>
                            </LinearLayout>
                            <ImageView
                                android:src="@drawable/drop_btn_take_a_photo"
                                android:contentDescription="[AID]ChatAdd_Camera"
                                style="@style/ChatGroupDialogSelectionImage"/>
                        </RelativeLayout>
                        <RelativeLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/voiceMessage"
                            android:background="@drawable/image_selector_press_30_percentage_dark"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    app:guideline_height="125"
                                    app:guideline_width="268"/>
                                <TextView
                                    android:textColor="#000000"
                                    android:text="@string/voice_message"
                                    style="@style/PlusFragmentButtonText"/>
                            </LinearLayout>
                            <ImageView
                                android:src="@drawable/drop_btn_voice_message"
                                android:contentDescription="[AID]ChatAdd_Voice"
                                style="@style/ChatGroupDialogSelectionImage"/>
                        </RelativeLayout>
                    </LinearLayout>
                </com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout>
                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    app:guideline_height="492"
                    app:guideline_width="300">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:divider="@drawable/padding_chat_plus_row"
                        android:showDividers="middle">
                        <RelativeLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/photoLibrary"
                            android:background="@drawable/image_selector_press_30_percentage_dark"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    app:guideline_height="125"
                                    app:guideline_width="268"/>
                                <TextView
                                    android:textColor="#000000"
                                    android:text="@string/photo_library"
                                    style="@style/PlusFragmentButtonText"/>
                            </LinearLayout>
                            <ImageView
                                android:src="@drawable/drop_btn_choose_a_photo"
                                android:contentDescription="[AID]ChatAdd_PhotoLibary"
                                style="@style/ChatGroupDialogSelectionImage"/>
                        </RelativeLayout>
                        <RelativeLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/shareLocation"
                            android:background="@drawable/image_selector_press_30_percentage_dark"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    app:guideline_height="125"
                                    app:guideline_width="268"/>
                                <TextView
                                    android:textColor="#000000"
                                    android:text="@string/Location"
                                    style="@style/PlusFragmentButtonText"/>
                            </LinearLayout>
                            <ImageView
                                android:src="@drawable/drop_btn_share_location"
                                android:contentDescription="[AID]ChatAdd_ShareLocation"
                                style="@style/ChatGroupDialogSelectionImage"/>
                        </RelativeLayout>
                    </LinearLayout>
                </com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout>
                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    app:guideline_height="492"
                    app:guideline_width="300">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:divider="@drawable/padding_chat_plus_row"
                        android:showDividers="middle">
                        <RelativeLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/videoCapture"
                            android:background="@drawable/image_selector_press_30_percentage_dark"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    app:guideline_height="125"
                                    app:guideline_width="268"/>
                                <TextView
                                    android:textColor="#000000"
                                    android:text="@string/take_a_video"
                                    style="@style/PlusFragmentButtonText"/>
                            </LinearLayout>
                            <ImageView
                                android:src="@drawable/drop_btn_take_a_video"
                                android:contentDescription="[AID]ChatAdd_Recoder"
                                style="@style/ChatGroupDialogSelectionImage"/>
                        </RelativeLayout>
                        <RelativeLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/introduceFriend"
                            android:background="@drawable/image_selector_press_30_percentage_dark"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    app:guideline_height="125"
                                    app:guideline_width="268"/>
                                <TextView
                                    android:textColor="#000000"
                                    android:text="@string/introduce_friend"
                                    style="@style/PlusFragmentButtonText"/>
                            </LinearLayout>
                            <ImageView
                                android:src="@drawable/drop_btn_introduce_friend"
                                android:contentDescription="[AID]ChatAdd_IntroFriend"
                                style="@style/ChatGroupDialogSelectionImage"/>
                        </RelativeLayout>
                    </LinearLayout>
                </com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout>
                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    app:guideline_height="492"
                    app:guideline_width="300">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:divider="@drawable/padding_chat_plus_row"
                        android:showDividers="middle">
                        <RelativeLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/videoLibrary"
                            android:background="@drawable/image_selector_press_30_percentage_dark"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    app:guideline_height="125"
                                    app:guideline_width="268"/>
                                <TextView
                                    android:textColor="#000000"
                                    android:text="@string/choose_a_video"
                                    style="@style/PlusFragmentButtonText"/>
                            </LinearLayout>
                            <ImageView
                                android:src="@drawable/drop_btn_choose_a_video"
                                android:contentDescription="[AID]ChatAdd_VideoLibary"
                                style="@style/ChatGroupDialogSelectionImage"/>
                        </RelativeLayout>
                        <RelativeLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/selfDestruct"
                            android:background="@drawable/image_selector_press_30_percentage_dark"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    app:guideline_height="125"
                                    app:guideline_width="268"/>
                                <TextView
                                    android:textColor="#000000"
                                    android:text="@string/self_destruct"
                                    style="@style/PlusFragmentButtonText"/>
                            </LinearLayout>
                            <ImageView
                                android:src="@drawable/drop_btn_self_destruct"
                                android:contentDescription="[AID]ChatAdd_SelfDestruct"
                                style="@style/ChatGroupDialogSelectionImage"/>
                        </RelativeLayout>
                    </LinearLayout>
                </com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout>
                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                    android:id="@+id/verticalBlock5"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    app:guideline_height="492"
                    app:guideline_width="300">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:divider="@drawable/padding_chat_plus_row"
                        android:showDividers="middle">
                        <RelativeLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/scheduleSend"
                            android:background="@drawable/image_selector_press_30_percentage_dark"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    app:guideline_height="125"
                                    app:guideline_width="268"/>
                                <TextView
                                    android:textColor="#000000"
                                    android:text="@string/schedule_send"
                                    style="@style/PlusFragmentButtonText"/>
                            </LinearLayout>
                            <ImageView
                                android:src="@drawable/drop_btn_delay_send"
                                android:contentDescription="[AID]ChatAdd_DelaySend"
                                style="@style/ChatGroupDialogSelectionImage"/>
                        </RelativeLayout>
                        <RelativeLayout
                            android:gravity="center_horizontal"
                            android:background="@drawable/image_selector_press_30_percentage_dark"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">
                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">
                                <com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="0dp"
                                    app:guideline_height="125"
                                    app:guideline_width="268"/>
                                <TextView
                                    android:textColor="#000000"
                                    style="@style/PlusFragmentButtonText"/>
                            </LinearLayout>
                            <ImageView style="@style/ChatGroupDialogSelectionImage"/>
                        </RelativeLayout>
                    </LinearLayout>
                </com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout>
            </LinearLayout>
        </HorizontalScrollView>
    </com.cyberlink.you.widgetpool.common.FixedAspectRatioFrameLayout>
</LinearLayout>
