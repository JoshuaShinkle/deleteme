<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="top"
    android:orientation="vertical"
    android:id="@+id/disable_chat_window"
    android:background="#f3f3f3"
    android:fadingEdgeLength="@dimen/t10dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:layout_gravity="center_horizontal"
        android:id="@+id/image_disable_chat"
        android:layout_width="@dimen/t137dp"
        android:layout_height="@dimen/t120dp"
        android:layout_marginTop="@dimen/t40dp"
        android:layout_marginBottom="@dimen/t20dp"
        android:src="@drawable/webinar_text_disable_chat"/>
    <TextView
        android:textSize="14dp"
        android:textColor="#636363"
        android:layout_gravity="center_horizontal"
        android:id="@+id/textView"
        android:layout_width="@dimen/t260dp"
        android:layout_height="wrap_content"
        android:text="@string/wbn_disable_chat_desc"
        android:lineSpacingExtra="@dimen/t5dp"/>
</LinearLayout>
