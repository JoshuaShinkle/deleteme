<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#595959"
    android:layout_width="match_parent"
    android:layout_height="@dimen/t48dp">
    <ImageView
        android:id="@+id/top_logo"
        android:layout_width="@dimen/t30dp"
        android:layout_height="@dimen/t30dp"
        android:layout_marginLeft="@dimen/t9dp"
        android:src="@drawable/top_logo"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"/>
    <TextView
        android:textSize="@dimen/t16dp"
        android:textColor="#ffffff"
        android:gravity="center_vertical"
        android:id="@+id/app_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/t9dp"
        android:text="@string/u_webinar"
        android:layout_toRightOf="@+id/top_logo"
        android:layout_centerVertical="true"/>
</RelativeLayout>
