<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/view_item_meeting_keypad"/>
    <View
        android:id="@+id/fakeStatusBar"
        android:background="@color/transparent"
        android:layout_width="match_parent"
        android:layout_height="0dp"/>
    <View
        android:id="@+id/speakerBackground"
        android:background="@drawable/meeting_speaker_portrait_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <View
        android:id="@+id/viewPagerIndicatorBackground"
        android:background="@drawable/meeting_viewpager_indicator_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:alpha="0"/>
    <RelativeLayout
        android:id="@+id/desktopSharingHingPanel"
        android:background="#00000000"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:background="#00000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true">
            <View
                android:id="@+id/desktopSharingHingPanelBackground"
                android:background="@color/clm_bg"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
        </RelativeLayout>
        <RelativeLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:id="@+id/desktopSharingHintContainer"
            android:background="#00000000"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/t150dp"
            android:layout_centerHorizontal="true">
            <RelativeLayout
                android:id="@+id/desktopSharingHintPortrait"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/desktopSharingHintImage"
                    android:background="@drawable/screensharing_img"
                    android:layout_width="@dimen/t95dp"
                    android:layout_height="@dimen/t164dp"
                    android:layout_centerHorizontal="true"/>
                <TextView
                    android:textSize="@dimen/t21dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:id="@+id/desktopSharingDescriptionText"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/t100dp"
                    android:text="@string/clm_meeting_desktop_sharing_hint"
                    android:layout_below="@+id/desktopSharingHintImage"/>
                <Button
                    android:textSize="@dimen/t20dp"
                    android:id="@+id/desktopSharingStopButton"
                    android:layout_height="@dimen/t45dp"
                    android:text="@string/clm_meeting_desktop_sharing_stop_button"
                    android:layout_below="@+id/desktopSharingDescriptionText"
                    style="@style/StopDesktopSharingButton"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/desktopSharingHintLandscape"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/desktopSharingHintImageLandscape"
                    android:background="@drawable/screensharing_img"
                    android:layout_width="@dimen/t75dp"
                    android:layout_height="@dimen/t144dp"/>
                <TextView
                    android:textSize="@dimen/t21dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/t50dp"
                    android:text="@string/clm_meeting_desktop_sharing_hint"
                    android:layout_toRightOf="@+id/desktopSharingHintImageLandscape"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/floatingMenuHeader"
        android:background="@color/transparent"
        android:layout_below="@+id/fakeStatusBar"
        style="@style/TitleBar">
        <TextView
            android:textSize="@dimen/t17dp"
            android:textColor="#ffffff"
            android:ellipsize="end"
            android:gravity="center"
            android:id="@+id/meetingTitle"
            android:maxWidth="@dimen/t180dp"
            android:maxLines="1"
            style="@style/MeetingsHeaderTitle"/>
        <ImageView
            android:id="@+id/meetingLock"
            android:visibility="gone"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_marginRight="5dp"
            android:src="@drawable/meeting_lock"
            android:scaleType="centerInside"
            android:layout_toLeftOf="@+id/meetingTitle"
            android:layout_centerVertical="true"
            android:contentDescription="[AID]Meeting_Lock"/>
        <LinearLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:id="@+id/btn_meeting_participant"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true">
            <ImageView
                android:id="@+id/participant"
                android:layout_width="@dimen/t33dp"
                android:layout_height="@dimen/t20dp"
                android:layout_marginLeft="@dimen/t10dp"
                android:src="@drawable/btn_meeting_participant"/>
            <TextView
                android:textSize="@dimen/t13dp"
                android:textColor="#ffffff"
                android:id="@+id/meeting_participant_count"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/t5dp"
                android:text="0"
                android:layout_centerVertical="true"/>
        </LinearLayout>
        <ImageView
            android:id="@+id/btn_meeting_messages"
            android:layout_width="@dimen/t34dp"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/t10dp"
            android:src="@drawable/btn_meeting_chat_read"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
        <ImageView
            android:id="@+id/btn_meeting_camera_switch"
            android:layout_width="@dimen/t25dp"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/t15dp"
            android:src="@drawable/btn_meeting_camera_switch"
            android:layout_toLeftOf="@+id/btn_meeting_messages"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/fakeStatusBar">
        <com.rockerhieu.emojicon.EmojiconTextView
            android:textSize="@dimen/t20dp"
            android:gravity="center"
            android:id="@+id/meetingStatusTextView"
            android:layout_marginTop="@dimen/t70dp"
            android:text="@string/clm_status_connecting"
            android:layout_alignParentTop="true"
            style="@style/MeetingsText"/>
        <RelativeLayout
            android:id="@+id/meetingSpeakerInviteParticipantsArea"
            android:background="@drawable/image_selector_common_button"
            android:visibility="gone"
            android:layout_width="@dimen/t130dp"
            android:layout_height="@dimen/t35dp"
            android:layout_marginTop="@dimen/t33dp"
            android:layout_below="@+id/meetingStatusTextView"
            android:layout_centerHorizontal="true">
            <ImageView
                android:id="@+id/meetingInviteButton"
                android:layout_width="@dimen/t24dp"
                android:layout_height="@dimen/t17dp"
                android:layout_marginLeft="@dimen/t20dp"
                android:src="@drawable/meeting_invite"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="@dimen/t20dp"
                android:gravity="center"
                android:id="@+id/meetingInviteTextView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="@dimen/t10dp"
                android:text="@string/clm_invite_page_title"
                android:layout_toRightOf="@+id/meetingInviteButton"
                android:layout_centerInParent="false"
                style="@style/MeetingsText"/>
        </RelativeLayout>
    </RelativeLayout>
    <Button
        android:textSize="@dimen/t25dp"
        android:textStyle="bold"
        android:id="@+id/count_down_breakout_room"
        android:background="@drawable/img_timer_2x"
        android:visibility="gone"
        android:clickable="false"
        android:layout_width="@dimen/t50dp"
        android:layout_height="@dimen/t50dp"
        android:layout_marginTop="@dimen/t80dp"
        android:text="12"
        android:layout_marginEnd="@dimen/t30dp"
        android:layout_alignParentEnd="true"/>
    <ImageButton
        android:id="@+id/btn_breakout_room"
        android:background="@null"
        android:visibility="invisible"
        android:layout_width="@dimen/t50dp"
        android:layout_height="@dimen/t50dp"
        android:layout_marginTop="@dimen/t80dp"
        android:src="@drawable/btn_breakoutroom"
        android:scaleType="centerCrop"
        android:layout_marginStart="@dimen/t30dp"/>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/breakout_room_hint_view"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/t5dp"
        android:layout_toRightOf="@+id/btn_breakout_room"
        android:layout_alignTop="@+id/btn_breakout_room"
        android:layout_alignBottom="@+id/btn_breakout_room"
        android:layout_marginEnd="@dimen/t10dp">
        <ImageView
            android:layout_gravity="center_vertical"
            android:layout_width="@dimen/t20dp"
            android:layout_height="@dimen/t20dp"
            android:layout_marginRight="-3dp"
            android:src="@drawable/btn_br_hint_pointer"/>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <ImageView
                android:background="@drawable/btn_br_hint_body"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignLeft="@+id/breakout_room_hint_text"
                android:layout_alignRight="@+id/breakout_room_hint_text"/>
            <TextView
                android:textSize="@dimen/t16dp"
                android:textStyle="bold"
                android:gravity="center_vertical"
                android:id="@+id/breakout_room_hint_text"
                android:paddingLeft="@dimen/t10dp"
                android:paddingRight="@dimen/t10dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"/>
        </RelativeLayout>
    </LinearLayout>
    <TextView
        android:textSize="@dimen/t20dp"
        android:textColor="@color/white"
        android:gravity="center"
        android:id="@+id/extra_message"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/localRendererContainer"
        android:layout_below="@+id/btn_breakout_room"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/t25dp"
        android:layout_marginEnd="@dimen/t25dp"/>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/t25dp"
        android:layout_marginTop="@dimen/t40dp"
        android:layout_marginRight="@dimen/t25dp"
        android:layout_below="@+id/fakeStatusBar"
        android:layout_centerHorizontal="true">
        <ImageView
            android:layout_gravity="center"
            android:id="@+id/speakerRaiseHand"
            android:background="@drawable/meeting_raise_hand"
            android:visibility="gone"
            android:layout_width="@dimen/t20dp"
            android:layout_height="@dimen/t20dp"/>
        <com.rockerhieu.emojicon.EmojiconTextView
            android:textSize="@dimen/t17dp"
            android:textColor="#ffffff"
            android:ellipsize="end"
            android:layout_gravity="center"
            android:id="@+id/speakerName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/t5dp"
            android:maxLines="1"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:id="@+id/speakerMeetingRecording"
        android:paddingLeft="@dimen/t10dp"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/t60dp"
        android:layout_below="@+id/fakeStatusBar">
        <ImageView
            android:layout_gravity="center"
            android:background="@drawable/meeting_speaker_recording"
            android:layout_width="@dimen/t10dp"
            android:layout_height="@dimen/t10dp"/>
        <TextView
            android:textColor="@color/clm_speaker_recording"
            android:gravity="center"
            android:paddingLeft="@dimen/t8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/clm_meeting_recording"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/btn_meeting_laser_pointer_landscape"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/t20dp"
        android:layout_marginBottom="@dimen/t8dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true">
        <ImageView
            android:layout_width="@dimen/clm_meeting_button_size_desktop_share"
            android:layout_height="@dimen/clm_meeting_button_size_desktop_share"
            android:src="@drawable/btn_meeting_laser_pointer"/>
        <TextView
            android:id="@+id/meeting_laser_pointer_landscape_text"
            android:text="@string/clm_meeting_button_laser_pointer"
            style="@style/MeetingsButtonText"/>
    </LinearLayout>
    <LinearLayout
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/t8dp"
        android:layout_above="@+id/floatingBottomMenuContainerParent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/btn_meeting_laser_pointer_portrait"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/t20dp">
            <ImageView
                android:layout_width="@dimen/clm_meeting_button_size_desktop_share"
                android:layout_height="@dimen/clm_meeting_button_size_desktop_share"
                android:src="@drawable/btn_meeting_laser_pointer"/>
        </LinearLayout>
        <LinearLayout
            android:gravity="bottom"
            android:layout_gravity="bottom"
            android:orientation="horizontal"
            android:id="@+id/desktopMenuContainer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:visibility="invisible"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/t20dp">
                <ImageView
                    android:layout_width="@dimen/clm_meeting_button_size"
                    android:layout_height="@dimen/clm_meeting_button_size"/>
                <TextView
                    android:text="Fake"
                    style="@style/MeetingsButtonText"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:visibility="invisible"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/t20dp">
                <ImageView
                    android:layout_width="@dimen/clm_meeting_button_size"
                    android:layout_height="@dimen/clm_meeting_button_size"/>
                <TextView
                    android:text="Fake"
                    style="@style/MeetingsButtonText"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:visibility="invisible"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/t20dp">
                <ImageView
                    android:layout_width="@dimen/clm_meeting_button_size"
                    android:layout_height="@dimen/clm_meeting_button_size"/>
                <TextView
                    android:text="Fake"
                    style="@style/MeetingsButtonText"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:visibility="invisible"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_width="@dimen/clm_meeting_button_size"
                    android:layout_height="@dimen/clm_meeting_button_size"/>
                <TextView
                    android:text="Fake"
                    style="@style/MeetingsButtonText"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <RelativeLayout
        android:layout_gravity="bottom|right"
        android:id="@+id/localRendererContainer"
        android:background="#cc000000"
        android:layout_width="@dimen/clm_local_container_short_edge"
        android:layout_height="@dimen/clm_local_container_long_edge"
        android:layout_marginRight="@dimen/clm_local_container_margin_right"
        android:layout_marginBottom="@dimen/clm_local_container_margin_bottom"
        android:layout_above="@+id/floatingBottomMenuContainerParent"
        android:layout_alignParentRight="true">
        <FrameLayout
            android:layout_gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true">
            <org.webrtc.SurfaceViewRenderer
                android:layout_gravity="center"
                android:id="@+id/localRendererView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </FrameLayout>
        <ImageView
            android:id="@+id/localRendererRaiseHandIndicator"
            android:visibility="gone"
            android:layout_width="@dimen/t33dp"
            android:layout_height="@dimen/t33dp"
            android:layout_marginTop="@dimen/t5dp"
            android:layout_marginRight="@dimen/t5dp"
            android:src="@drawable/meeting_raise_hand"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"/>
        <ImageView
            android:id="@+id/localRendererVideoCloseIndicator"
            android:visibility="gone"
            android:layout_width="@dimen/t40dp"
            android:layout_height="@dimen/t43dp"
            android:src="@drawable/meeting_video_off_indicator"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/localRendererMuteIndicator"/>
        <ImageView
            android:id="@+id/localRendererMuteIndicator"
            android:visibility="gone"
            android:layout_width="@dimen/t40dp"
            android:layout_height="@dimen/t43dp"
            android:src="@drawable/meeting_mute_indicator"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"/>
    </RelativeLayout>
    <FrameLayout
        android:layout_gravity="bottom|right"
        android:id="@+id/desktopCameraContainer"
        android:layout_width="@dimen/clm_local_container_long_edge"
        android:layout_height="@dimen/clm_local_container_short_edge"
        android:layout_marginRight="@dimen/clm_local_container_margin_right"
        android:layout_marginBottom="@dimen/clm_local_container_margin_bottom"
        android:layout_above="@+id/floatingBottomMenuContainerParent"
        android:layout_alignParentRight="true">
        <FrameLayout
            android:id="@+id/desktopCameraAvatarContainer"
            android:background="@color/clm_gallery_background"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ImageView
                android:layout_gravity="center"
                android:id="@+id/desktopCameraAvatar"
                android:layout_width="@dimen/clm_meeting_desktop_avatar"
                android:layout_height="@dimen/clm_meeting_desktop_avatar"
                style="@style/AvatarImageView"/>
        </FrameLayout>
        <FrameLayout
            android:id="@+id/desktopCameraVideoContainer"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </FrameLayout>
    <LinearLayout
        android:gravity="center_horizontal"
        android:id="@+id/floatingBottomMenuContainerParent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/t8dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:gravity="bottom"
            android:layout_gravity="bottom"
            android:id="@+id/floatingBottomMenuContainer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/btn_meeting_keypad"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/t20dp">
                <ImageView
                    android:layout_width="@dimen/t60dp"
                    android:layout_height="@dimen/t60dp"
                    android:src="@drawable/btn_meeting_key_pad"/>
                <TextView
                    android:text="@string/clm_meeting_button_key_pad"
                    style="@style/MeetingsButtonText"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/btn_meeting_camera"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/t20dp">
                <ImageView
                    android:layout_width="@dimen/clm_meeting_button_size"
                    android:layout_height="@dimen/clm_meeting_button_size"
                    android:src="@drawable/btn_meeting_camera"/>
                <TextView
                    android:id="@+id/meeting_camera_text"
                    android:text="@string/clm_meeting_button_video"
                    style="@style/MeetingsButtonText"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/btn_meeting_microphone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/t20dp">
                <ImageView
                    android:layout_width="@dimen/clm_meeting_button_size"
                    android:layout_height="@dimen/clm_meeting_button_size"
                    android:src="@drawable/btn_meeting_microphone"/>
                <TextView
                    android:id="@+id/meeting_microphone_text"
                    android:text="@string/clm_meeting_button_mute"
                    style="@style/MeetingsButtonText"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/btn_meeting_speaker"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/t20dp">
                <ImageView
                    android:layout_width="@dimen/clm_meeting_button_size"
                    android:layout_height="@dimen/clm_meeting_button_size"
                    android:src="@drawable/btn_meeting_speaker"/>
                <TextView
                    android:id="@+id/meeting_speaker_text"
                    android:text="@string/clm_meeting_button_speaker"
                    style="@style/MeetingsButtonText"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/btn_meeting_more"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/t20dp">
                <ImageView
                    android:id="@+id/btn_meeting_more_image"
                    android:layout_width="@dimen/clm_meeting_button_size"
                    android:layout_height="@dimen/clm_meeting_button_size"
                    android:src="@drawable/btn_meeting_more"/>
                <TextView
                    android:id="@+id/meeting_more_text"
                    android:text="@string/more_fragment"
                    style="@style/MeetingsButtonText"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/btn_meeting_hang_up"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageView
                    android:layout_width="@dimen/clm_meeting_button_size"
                    android:layout_height="@dimen/clm_meeting_button_size"
                    android:src="@drawable/btn_meeting_hang_up"/>
                <TextView
                    android:id="@+id/meeting_hang_up_text"
                    android:text="@string/clm_meeting_button_hang_up"
                    style="@style/MeetingsButtonText"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <TextView
        android:textSize="@dimen/t18dp"
        android:textColor="#ffffff"
        android:id="@+id/speakerTimer"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/t21dp"
        android:layout_marginBottom="@dimen/t8dp"
        android:layout_above="@+id/floatingBottomMenuContainerParent"
        android:layout_centerHorizontal="true"/>
    <LinearLayout
        android:gravity="center"
        android:orientation="horizontal"
        android:id="@+id/viewPagerIndicator"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/t42dp"
        android:layout_above="@+id/floatingBottomMenuContainerParent">
        <ImageView
            android:layout_width="@dimen/t15dp"
            android:layout_height="@dimen/t15dp"
            android:src="@drawable/meeting_indicator_dot"/>
        <ImageView
            android:layout_width="@dimen/t15dp"
            android:layout_height="@dimen/t15dp"
            android:layout_marginLeft="@dimen/t8dp"
            android:src="@drawable/meeting_indicator_dot"/>
    </LinearLayout>
</RelativeLayout>
