<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:textColor="@color/bc_color_red"
            android:gravity="center"
            android:id="@+id/item_return_to_main"
            android:text="@string/breakout_room_leave_dialog_return_to_main"
            style="@style/DialogSelectionItemText"/>
        <View
            android:background="@color/you_color_normal_gray"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:textColor="@color/bc_color_red"
            android:gravity="center"
            android:id="@+id/item_leave_meeting"
            android:text="@string/clm_leave_meeting_dialog_leave"
            style="@style/DialogSelectionItemText"/>
        <View
            android:background="@color/you_color_normal_gray"
            android:layout_width="match_parent"
            android:layout_height="1px"/>
    </LinearLayout>
</LinearLayout>
